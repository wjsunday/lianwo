{
  "name": "react-swipeable-views",
  "version": "0.8.0",
  "description": "A React component for swipeable views",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . && echo \"eslint: no lint errors\"",
    "test": "npm run lint && npm run test:unit && npm run flow",
    "test:unit": "NODE_ENV=release babel-node test/unit.js",
    "test:unit:watch": "NODE_ENV=release babel-node test/watch.js",
    "build": "rm -rf lib && mkdir lib && NODE_ENV=release babel src --out-dir lib --ignore spec.js",
    "docs:browser:development": "cd docs && NODE_ENV=docs-browser-development babel-node webpack.js --dev",
    "docs:browser:production": "cd docs && NODE_ENV=docs-browser-production babel-node webpack.js",
    "docs:native:development": "NODE_ENV=docs-native-development node node_modules/react-native/local-cli/cli.js start --reset-cache --verbose",
    "prepublish": "npm run build && pkgfiles",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oliviertassinari/react-swipeable-views.git"
  },
  "keywords": [
    "react",
    "react-component",
    "react-native",
    "swipe",
    "swipeable",
    "slider"
  ],
  "author": "Olivier Tassinari <olivier.tassinari@gmail.com> (https://github.com/oliviertassinari)",
  "bugs": {
    "url": "https://github.com/oliviertassinari/react-swipeable-views/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "keycode": "^2.1.7",
    "react-addons-shallow-compare": "^0.14.0 || ^15.0.0",
    "react-event-listener": "^0.4.0",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^0.14.0"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.33.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.24.0",
    "ip": "^1.0.1",
    "jsdom": "^9.8.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.1",
    "material-ui": "^0.16.1",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "pkgfiles": "^2.3.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-motion": "^0.4.5",
    "react-native": "^0.36.0",
    "react-native-button": "^1.7.1",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.20.2",
    "rimraf": "^2.5.4",
    "stats-webpack-plugin": "^0.4.2",
    "style-loader": "^0.13.0",
    "unused-files-webpack-plugin": "^3.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
